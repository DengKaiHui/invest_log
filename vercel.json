{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
  "buildCommand": "",
  "outputDirectory": ".",
  "env": {
    "FINNHUB_KEY": "@finnhub-key",
    "ALPHA_VANTAGE_KEY": "@alphavantage-key"
  }
}
